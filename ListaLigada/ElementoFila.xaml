<UserControl x:Class="ListaLigada.ElementoFila"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="54" Width="54" Name="elFila">
    <Grid>
        <Image Source="Imagens\Carrinho.png" Opacity="0.6"/>
        <ContentPresenter TextElement.FontSize="14" TextElement.FontWeight="Bold"
              HorizontalAlignment="Center" VerticalAlignment="Center"
              Content="{Binding ElementName=elFila, Path=Conteudo, Mode=TwoWay}"/>

    </Grid>
</UserControl>
